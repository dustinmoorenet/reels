{
  "name": "reels",
  "version": "1.0.0",
  "description": "Home movie website",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=./client mocha \"client/js/tests/**/*.js\" --reporter spec --compilers js:babel-register",
    "build:css": "node-sass ./client/js/sass/index.scss ./dist/index.css",
    "watch:css": "npm run build:css && node-sass --watch --source-map=true ./client/js/sass/index.scss ./dist/index.css",
    "build:js": "NODE_ENV=production webpack -p -d --bail",
    "watch:js": "webpack -d --watch",
    "build:html": "mkdir -p dist && cp client/index.html dist && cp client/favicon.ico dist",
    "build:scripts": "BABEL_ENV=node webpack -d --config webpack.node.config.js",
    "watch:scripts": "BABEL_ENV=node webpack -d --config webpack.node.config.js --watch",
    "build": "npm run build:css && npm run build:js && npm run build:html",
    "watch": "npm run watch:css & npm run watch:js & npm run build:html",
    "serve": "node scripts/serve.js",
    "start": "npm run watch & npm run serve",
    "publish": "aws s3 sync dist s3://movies.mooredavisgenealogy.com --acl public-read"
  },
  "repository": {
    "type": "git",
    "url": "ssh://dustin@server/disk1/git/reels.git"
  },
  "author": "Dustin Moore",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-class-properties": "6.5.2",
    "babel-plugin-transform-object-rest-spread": "6.5.0",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.5.2",
    "babel-runtime": "6.5.0",
    "classnames": "2.2.3",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.1",
    "eslint-plugin-react": "4.0.0",
    "expect": "^1.14.0",
    "express": "4.13.4",
    "google-spreadsheet": "1.1.4",
    "lodash": "4.17.11",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "nodemon": "1.9.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "redux": "3.3.1",
    "redux-devtools": "3.1.1",
    "redux-logger": "2.5.2",
    "redux-thunk": "1.0.3",
    "reselect": "2.0.3",
    "webpack": "1.12.13"
  }
}
